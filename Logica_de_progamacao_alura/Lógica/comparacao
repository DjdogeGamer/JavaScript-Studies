if n1 > n2 and n1 > n3:
    m = n1
else:
    mn = n1

if n2 > n3 and n2 > n1:
    m = n2
else:
    mn = n2

if n3 > n1 and n3 > n2:
    m = n3
else:
    mn = n3

#########

if n3 > n2 and n3 > n1:
    m = n3
else:
    mn = n3

if n1 < n2 < n3:
    m = n2
else:
    mn = n2

if n1 > n3 and n1 > n2:
    m = n1
else:
    mn = n1

#########

